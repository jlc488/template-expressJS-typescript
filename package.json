{
  "name": "expresjs-skeleton-using-typescript",
  "version": "1.0.0",
  "description": "expressjs skeleton using typescript",
  "main": "ts-build/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node ./src/server/index.ts",
    "start": "nodemon ./ts-build/index.js",
    "prod": "npm run build && npm run start",
    "lint": "tslint 'src/**/*.{ts}'",
    "local:db": "node_modules/json-server/bin/index.js  --watch db/db.json"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.16.0",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "nodemon": "^1.18.9"
  },
  "devDependencies": {
    "json-server": "^0.14.0",
    "prettier": "^1.15.3",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.2.2"
  }
}
